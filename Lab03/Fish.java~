  }//end printVerse1
  public static void printVerse2(){
    String verse2 = "Black fish\nBlue fish\nOld fish\nNew fish.\n";
    System.out.println(verse2);
  }//end printVerse2
  public static void printVerse(String verse){
    System.out.println(verse);
  }//end printVerse
